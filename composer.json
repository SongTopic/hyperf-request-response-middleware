{
    "name": "palmbuy/hyperf-request-response-middleware",
    "description": "A middleware that records request and response data",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "type": "package",
    "license": "MIT",
    "require": {
        "php": ">=5.5.9",
        "wufly/logstash": "^1.0"
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "1.0-dev"
        },
        "hyperf": {
            "config": "Palmbuy\\Log\\RequestLogServiceProvider"
        }
    },
    "autoload": {
        "psr-4": {
            "Palmbuy\\Log\\": "src/"
        }
    }
}
